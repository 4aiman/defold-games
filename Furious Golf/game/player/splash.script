function on_message(self, message_id, message, sender)
	if message_id == hash("water") then
		go.set_position(message.pos)
		particlefx.play("#water")
	elseif message_id == hash("lava") then
		go.set_position(message.pos)
		particlefx.play("#lava")
	elseif message_id == hash("slime") then
		go.set_position(message.pos)
		particlefx.play("#slime")
	end
end

